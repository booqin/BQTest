<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.administrator.bqtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bq_ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".BQApplication"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:theme, android:supportsRtl, android:label">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.frescotest.BQMainActivity"/>

        <activity android:name="com.example.openimtest.OpenIMTestActivity"/>

        <activity android:name="com.example.domainsetting.DomainSettingActivity"/>

    </application>

</manifest>